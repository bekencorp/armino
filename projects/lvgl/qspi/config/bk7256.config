#
# Compile/Build
#
#CONFIG_TOOLCHAIN_PATH="/opt/risc-v/nds32le-elf-mculib-v5/bin"
CONFIG_LV_QSPI_DEMO=y
CONFIG_LVGL=y
CONFIG_TP=n
CONFIG_LV_FONT_MONTSERRAT_12=y
CONFIG_LV_FONT_MONTSERRAT_14=y
CONFIG_LV_FONT_MONTSERRAT_24=y
CONFIG_LV_COLOR_DEPTH=16
CONFIG_LV_USE_PERF_MONITOR=n
CONFIG_LV_ATTRIBUTE_FAST_MEM=n
CONFIG_LVGL_USE_PSRAM=n

CONFIG_CACHE_CUSTOM_SRAM_MAPPING=n
CONFIG_CACHE_ENABLE=n
CONFIG_OVERRIDE_FLASH_PARTITION=y
CONFIG_LCD_QSPI=y
CONFIG_LCD_QSPI_TEST=y
CONFIG_LCD_QSPI_ST77903_SAT61478M=y
CONFIG_LCD_QSPI_PIXEL_DEPTH_BYTE=2
## CONFIG_SLAVE_CORE_OFFSET 2162688 == 0x210000 in bk7256_cp1_bsp.ld
CONFIG_SLAVE_CORE_OFFSET=2162688
CONFIG_CUSTOMIZE_HEAP_SIZE=262144

CONFIG_IPERF_TEST=n
CONFIG_JPEGENC_HW=n
CONFIG_JPEG_ENCODE=n
CONFIG_CAMERA=n
CONFIG_CAMERA_TEST=n
CONFIG_DOORBELL=n
CONFIG_PSRAM_TEST=n
#
# USB configuration
#
CONFIG_USB=n

#
# USB
#
CONFIG_USB_HOST=n
# CONFIG_USB_DEVICE is not set
# CONFIG_USB1_PORT is not set
#CONFIG_TASK_USB_PRIO=5 is not set
CONFIG_USB_MSD=n
# CONFIG_USB_HID is not set
# CONFIG_USB_CCD is not set
# CONFIG_USB_UVC is not set
# CONFIG_USB_CHARGE is not set
# end of USB configuration

#
# SDCARD
#
CONFIG_SDCARD_HOST=n
CONFIG_SDCARD_BUSWIDTH_4LINE=n
# end of SDCARD

#
# FATFS
#
CONFIG_FATFS=n
# end of FATFS