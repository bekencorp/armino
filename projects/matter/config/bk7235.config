#
# Compile/Build
#
CONFIG_TOOLCHAIN_PATH="/opt/risc-v/nds32le-elf-mculib-v5/bin"

CONFIG_MATTER_START=y
CONFIG_MBEDTLS=y
CONFIG_LWIP_V2_1=y
CONFIG_IPV6=y

CONFIG_SUPPORT_MATTER=y

#
# P2P 
#
CONFIG_P2P=n
CONFIG_P2P_GO=n
CONFIG_WIFI_WPS=n
CONFIG_WPS=n
#
#end of P2P

#
# Video transfer
#
CONFIG_JPEGENC_HW=n
CONFIG_CAMERA=n
CONFIG_USE_APP_DEMO_VIDEO_TRANSSER=n
CONFIG_APP_DEMO_VIDEO_TRANSFER=n
CONFIG_NET_WORK_VIDEO_TRANSFER=n
CONFIG_CAMERA_USE_I2C0=n
#
#end of Video transfer
